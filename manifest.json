{
    "name": "Web Carbon Calculator",
    "description": "Displays the carbon emissions of the website you're visiting.",
    "version": "0.0.2",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "*://*.google.com/*"
    ]
}